ENTRY(start_asm)

MEMORY
{
    ram : ORIGIN = 0x8000, LENGTH = 0x100000
}

SECTIONS
{
    .text : { *(.text*) } > ram
    .data : { *(.data*) } > ram
    .ARM.attributes : { *(.ARM.attributes*) } > ram
    __comment__ = .;
    .comment : { *(.comment*) } > ram
    __bss_start__ = .;
    .bss : { *(.bss*) } > ram
    __bss_end__ = .;
    _freeMemoryStart = .;
}
